<div class="modal fade" id="modalRegistar">
    <div class="modal-dialog modal-lg">
        <div class="modal-content" >
            <div class="modalConteinerR">
                <div class="modalLeftR">
                </div>
                <div class="modalRightR">
                    <button type="button" class="close" style="margin-bottom: 0;margin-top: 7%;margin-right: 7%;" data-dismiss="modal">&times;</button>
                    <div class="modalFormBoxR">
                        <form action="/registar" method="POST">
                            <p style="margin-bottom: 16px;">Primeiro Nome</p>
                            <input type="text" name="registarPrimeiroNome" placeholder="Primeiro Nome" >
                            <p style="margin-bottom: 16px;">Ultimo Nome</p>
                            <input type="text" name="registarUltimoNome" placeholder="Ultimo Nome" required>
                            <p style="margin-bottom: 16px;">Email</p>
                            <input type="email" name="registarEmail" placeholder="Email" required>
                            <p style="margin-bottom: 16px;">Género</p>
                            <select name="registarGenero" aria-placeholder="Género" required>
                                <option value="">&nbsp;</option>
                                <option value="mas">Masculino</option>
                                <option value="fem">Feminino</option>
                                <option value="outro">Outro</option>
                            </select>
                            <p>Password</p>
                            <div class="olho2"><i class="fa fa-eye-slash" aria-hidden="true"></i></div>
                            <input class="senha2" type="password" name="registarPassword" placeholder="Password" required>
                            <p>Confirmar Password</p>
                            <div class="olho3"><i class="fa fa-eye-slash" aria-hidden="true"></i></div>
                            <input class="senha3" type="password" name="registarConfirmarPassword" placeholder="Confirmar Password" required>
                            <input type="submit" name="registarSubmit" value="Registar">
                        </form>
                        <a href="#" data-target="#modalLogin" data-toggle="modal">Já tem conta?</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>